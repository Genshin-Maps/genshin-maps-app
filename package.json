{
  "name": "genshin-maps-app",
  "version": "1.0.0",
  "description": "Gamedot genshin maps app using electron.",
  "main": "out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "author": "juhyeon-cha",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.5",
    "lodash": "^4.17.21",
    "ref-array-di": "^1.2.2"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^1.0.2",
    "@electron/notarize": "^1.2.3",
    "@lwahonen/ffi-napi": "^4.0.12",
    "@lwahonen/ref-napi": "^4.0.8",
    "@sveltejs/vite-plugin-svelte": "^2.1.1",
    "@types/ffi-napi": "^4.0.7",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.2",
    "@types/ref-array-di": "^1.2.5",
    "@types/ref-napi": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^24.1.3",
    "electron-builder": "^23.6.0",
    "electron-updater": "^5.3.0",
    "electron-vite": "^1.0.22",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.8",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  }
}
